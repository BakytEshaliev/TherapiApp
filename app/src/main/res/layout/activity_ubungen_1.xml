<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/toolbar_3" />

    <include
        android:id="@+id/include16"
        layout="@layout/toolbar_2" />
    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="196dp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        android:text="Übungsname"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView73"
        app:layout_constraintTop_toBottomOf="@+id/textView73" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="196dp"
        android:layout_height="42dp"
        android:layout_marginTop="32dp"
        android:text="Übungsname"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView75"
        app:layout_constraintTop_toBottomOf="@+id/textView75" />

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="196dp"
        android:layout_height="42dp"
        android:layout_marginTop="24dp"
        android:text="Übungsname"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView74"
        app:layout_constraintTop_toBottomOf="@+id/textView74" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="196dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:text="Übungsname"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/checkBox4"
        app:layout_constraintTop_toBottomOf="@+id/checkBox4" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="196dp"
        android:layout_height="42dp"
        android:layout_marginTop="32dp"
        android:text="Übungsname"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/checkBox5"
        app:layout_constraintTop_toBottomOf="@+id/checkBox5" />

    <TextView
        android:id="@+id/textView73"
        android:layout_width="118dp"
        android:layout_height="37dp"
        android:layout_marginTop="32dp"
        android:text="Tagesziel"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include16" />

    <TextView
        android:id="@+id/textView74"
        android:layout_width="192dp"
        android:layout_height="39dp"
        android:layout_marginTop="24dp"
        android:text="Offene Übungen"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/checkBox"
        app:layout_constraintTop_toBottomOf="@+id/checkBox" />

    <TextView
        android:id="@+id/textView75"
        android:layout_width="294dp"
        android:layout_height="55dp"
        android:layout_marginTop="32dp"
        android:text="Abgeschlossene Übungen"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/checkBox3"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3" />

</androidx.constraintlayout.widget.ConstraintLayout>